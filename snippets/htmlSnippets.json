{
  "HTML boilerplate code": {
    "prefix": "!!",
    "body": [
      "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n\t<meta charset=\"UTF-8\">\n\t<meta name=\"viewport\" content= \"width=device-width, initial-scale=1.0, shrink-to-fit=no\">\n\t<meta http-equiv=\"X-UA-Compatible\" content=\"ie=edge\">\n\t<meta http-equiv=\"Cache-Control\" content=\"no-cache\">\n\t<meta http-equiv=\"expires\" content=\"0\">\n\t<meta http-equiv=\"Pragma\" content=\"no-cache\">\n\t<title>${1:Document}</title>\n\t<link rel=\"stylesheet\" href=\"styles/style.css\">\n</head>\n<body>\n\t<h1>${2:heading}</h1>\n</body>\n</html>"
    ]
  },
  "Primary button": {
    "prefix": "b:p",
    "body": "<button type=\"button\" class=\"btn btn-primary\" id=\"${1:btnSubmit}\" data-i18n=\"${2:btn.label.submit}\"></button>"
  },
  "Secondary button": {
    "prefix": "b:s",
    "body": "<button type=\"button\" class=\"btn btn-secondary\" id=\"${1:btnCancel}\" data-i18n=\"${2:btn.label.cancel}\"></button>"
  },
  "Image": {
    "prefix": ":img",
    "body": "<img src=\"${1:src}\" alt=\"${2:alt text}\">"
  },
  "Empty Anchor tag": {
    "prefix": ":a",
    "body": "<a href=\"javascript:;\" role=\"button\"></a>"
  },
  "Text field": {
    "prefix": "i:t",
    "body": "<label for=\"${1:firstName}\">${2:First Name}</label>\n<input type=\"text\" id=\"${3:firstName}\" placeholder=\"${4:Enter your Name}\">"
  },
  "Email field": {
    "prefix": "i:e",
    "body": "<label for=\"${1:email}\">${2:Email}</label>\n<input type=\"email\" id=\"email\" placeholder=\"${1:Enter your Email}\">"
  },
  "Password field": {
    "prefix": "i:p",
    "body": "<label for=\"${1:password}\">${2:Password}</label>\n<input type=\"password\" id=\"password\" placeholder=\"${1:Enter your Password}\">"
  },
  "Modal Content": {
    "prefix": ":modal",
    "body": "<div class=\"${1:container-component} modal-dialog\" role=\"document\" id=\"c_${2:id}\">\n\t<div class=\"modal-content\">\n\t\t<div class=\"modal-header\">\n\t\t\t<h5 class=\"modal-title\">Modal title</h5>\n\t\t</div>\n\t\t<div class=\"modal-body\">Modal body</div>\n\t\t<div class=\"modal-footer\">\n\t<button type=\"button\" data-dismiss=\"modal\">Close</button>\n\t</div>\n\t</div>\n</div>"
  },
  "Base layout": {
    "prefix": ":l",
    "body": "<main>\n\t<header></header>\n\t<article></article>\n\t<footer></footer>\n</main>"
  },
  "Page layout": {
    "prefix": "p:l",
    "body": "<section class=\"page-content\">\n\t<div class=\"page-header\"></div>\n\t<div class=\"page-body\"></div>\n\t<div class=\"page-footer\"></div>\n</section>"
  },
  "Icon": {
    "prefix": ":i",
    "body": "<i class=\"icon\"></i>"
  }
}